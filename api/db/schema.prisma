datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider      = "prisma-client-js"
  binaryTargets = "native"
}

model Tag {
  uuidTag  String @id @default(uuid())
  tagTitleNormalized  String @unique
  tagTitleWithDiacritics String?
  tagDescription String?
  tagWeight Int
}
