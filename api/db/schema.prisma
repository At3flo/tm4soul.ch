datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider      = "prisma-client-js"
  binaryTargets = "native"
}

model GalleryDescription {
  uuidGalleryDescription  String @id @default(uuid())
  galleryDescriptionHeader  String?
  galleryDescriptionContent String?
}
